{
  "extends": [
    "tslint:recommended",
    "tslint-angular",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "template-no-negated-async": false,
    "no-non-null-assertion": false,
    "forin": false,
    "max-classes-per-file": false,
    "no-empty-interface": false
  }
}
